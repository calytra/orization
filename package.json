{
	"name": "@calytra/orization",
	"version": "0.0.4",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js"
	},
	"type": "module",
	"scripts": {
		"build": "tsup ./src/index.ts --format esm --dts-resolve --sourcemap --dts",
		"dev": "tsup ./src/index.ts --format esm --dts-resolve --sourcemap --dts --watch",
		"lint": "eslint . && prettier -c src",
		"lint:fix": "eslint . --fix && prettier -w src",
		"release": "pnpm test && pnpm build && changelogen --release --push && npm publish",
		"test": "pnpm lint && pnpm vitest run --coverage"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "^2.1.1",
		"changelogen": "^0.5.7",
		"eslint": "^9.12.0",
		"eslint-config-unjs": "^0.4.1",
		"nanoid": "^5.0.7",
		"prettier": "^3.3.2",
		"tsup": "^8.2.4",
		"typescript": "^5.6.3",
		"vitest": "^2.1.1"
	},
	"packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
